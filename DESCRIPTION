Package: stablehlo
Title: Write stableHLO programs
Version: 0.0.0.9000
Authors@R: c(
    person("Daniel", "Falbel", , "daniel@posit.co", role = c("aut", "cre"),
           comment = c(ORCID = "https://orcid.org/0009-0006-0143-2392")),
